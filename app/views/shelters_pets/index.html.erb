<h1><%= link_to "Shelter List", "/shelters" %></h1><br></br>
<h2><%= link_to "Pet List", "/pets" %></h2><br></br>
<h3>Adoptable Pets Here!</h3>

<% @pets.each do |pet| %>
  <img src= <%= pet.image %>>
  <p><%= link_to pet.name, "/pets/#{pet.id}" %></p>
  <p><%= pet.approx_age %></p>
  <p><%= pet.sex %></p>
<% end %>

<p><%= link_to "Create Pet", "/shelters/#{@shelter.id}/pets/new"  %></p>
