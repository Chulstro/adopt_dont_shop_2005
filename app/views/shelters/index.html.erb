<h1><%= link_to "Shelter List", "/shelters" %></h1><br></br>
<h2><%= link_to "Pet List", "/pets" %></h2><br></br>
<h3>All Shelters</h3>

<% @shelters.each do |shelter| %>
  <h2><%= link_to shelter.name, "/shelters/#{shelter.id}" %></h2>
  <p><%= link_to "Edit #{shelter.name}", "/shelters/#{shelter.id}/edit" %></p>
  <p><%= link_to "Delete #{shelter.name}", "/shelters/#{shelter.id}", method: :delete %></p>
<% end %>

<%= link_to "New Shelter", "/shelters/new" %>
